<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00FFFFFF"
    tools:context=".Detail">

    <View
        android:id="@+id/view3"
        android:layout_width="350dp"
        android:layout_height="4px"
        android:layout_marginStart="30.5dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="30.5dp"
        android:background="#C3A17F"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="139dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="139dp"
        android:letterSpacing="0.2"
        android:text="Detail"
        android:textColor="#C3A17F"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="335dp"
        android:layout_height="215dp"
        android:layout_marginTop="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view3"
        app:srcCompat="@drawable/image_rec" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="350dp"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4">

        <TextView
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            android:letterSpacing="0.1"
            android:text="Name"
            android:textColor="#C3A17F"
            android:textSize="20dp" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1" />


        <TextView
            android:id="@+id/name"
            android:layout_width="180dp"
            android:layout_height="30dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="20dp"
            android:gravity="right"
            android:letterSpacing="0.1"
            android:text="Name"
            android:textColor="#C3A17F"
            android:textSize="20dp" />

    </LinearLayout>

    <View
        android:id="@+id/view5"
        android:layout_width="350dp"
        android:layout_height="4px"
        android:layout_marginRight="30dp"
        android:background="#C3A17F"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2" />

    <LinearLayout
        android:id="@+id/linearLayout8"
        android:layout_width="350dp"
        android:layout_height="45dp"
        android:layout_marginTop="65dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4">

        <TextView
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            android:letterSpacing="0.1"
            android:text="Amount"
            android:textColor="#C3A17F"
            android:textSize="20dp" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1" />


        <TextView
            android:id="@+id/amount"
            android:layout_width="180dp"
            android:layout_height="30dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="20dp"
            android:gravity="right"
            android:letterSpacing="0.1"
            android:text="Amount"
            android:textColor="#C3A17F"
            android:textSize="20dp" />

    </LinearLayout>

    <View
        android:id="@+id/view6"
        android:layout_width="350dp"
        android:layout_height="4px"
        android:layout_marginRight="30dp"
        android:background="#C3A17F"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout8" />

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="350dp"
        android:layout_height="45dp"
        android:layout_marginTop="120dp"
        android:background="@drawable/review_rec"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4">

        <TextView
            android:id="@+id/review"
            android:layout_width="314dp"
            android:layout_height="30dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="7dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="5dp"
            android:letterSpacing="0.1"
            android:text="Review"
            android:textColor="#C3A17F"
            android:textSize="20dp" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout9"
        android:layout_width="350dp"
        android:layout_height="45dp"
        android:layout_marginTop="175dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4">

        <TextView
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            android:letterSpacing="0.1"
            android:text="Date"
            android:textColor="#C3A17F"
            android:textSize="20dp" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1" />


        <TextView
            android:id="@+id/date"
            android:layout_width="180dp"
            android:layout_height="30dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="20dp"
            android:gravity="right"
            android:letterSpacing="0.1"
            android:text="Date"
            android:textColor="#C3A17F"
            android:textSize="20dp" />

    </LinearLayout>

    <View
        android:id="@+id/view7"
        android:layout_width="350dp"
        android:layout_height="4px"
        android:layout_marginRight="30dp"
        android:background="#C3A17F"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout9" />

    <LinearLayout
        android:id="@+id/linearLayout10"
        android:layout_width="350dp"
        android:layout_height="45dp"
        android:layout_marginTop="230dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4">

        <TextView
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            android:letterSpacing="0.1"
            android:text="Time"
            android:textColor="#C3A17F"
            android:textSize="20dp" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1" />


        <TextView
            android:id="@+id/time"
            android:layout_width="180dp"
            android:layout_height="30dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="20dp"
            android:gravity="right"
            android:letterSpacing="0.1"
            android:text="Time"
            android:textColor="#C3A17F"
            android:textSize="20dp" />

    </LinearLayout>

    <View
        android:id="@+id/view8"
        android:layout_width="350dp"
        android:layout_height="4px"
        android:layout_marginRight="30dp"
        android:background="#C3A17F"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout10" />

    <LinearLayout
        android:id="@+id/linearLayout11"
        android:layout_width="350dp"
        android:layout_height="45dp"
        android:layout_marginTop="285dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4">

        <TextView
            android:layout_width="80dp"
            android:layout_height="30dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            android:letterSpacing="0.1"
            android:text="Place"
            android:textColor="#C3A17F"
            android:textSize="20dp" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1" />


        <TextView
            android:id="@+id/place"
            android:layout_width="230dp"
            android:layout_height="30dp"
            android:layout_marginTop="2dp"
            android:layout_marginRight="20dp"
            android:gravity="right"
            android:letterSpacing="0.1"
            android:text="Place"
            android:textColor="#C3A17F"
            android:textSize="15dp" />

    </LinearLayout>

    <View
        android:id="@+id/view9"
        android:layout_width="350dp"
        android:layout_height="4px"
        android:layout_marginRight="30dp"
        android:background="#C3A17F"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout11" />


    <LinearLayout
        android:id="@+id/linearLayout12"
        android:layout_width="350dp"
        android:layout_height="45dp"
        android:layout_marginTop="340dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4">

        <TextView
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            android:letterSpacing="0.1"
            android:text="Calories"
            android:textColor="#C3A17F"
            android:textSize="20dp" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1" />


        <TextView
            android:id="@+id/calories"
            android:layout_width="180dp"
            android:layout_height="30dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="20dp"
            android:gravity="right"
            android:letterSpacing="0.1"
            android:text="Calories"
            android:textColor="#C3A17F"
            android:textSize="20dp" />

    </LinearLayout>

    <View
        android:id="@+id/view10"
        android:layout_width="350dp"
        android:layout_height="4px"
        android:layout_marginRight="30dp"
        android:background="#C3A17F"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout12" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="36dp"
        android:layout_height="34dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="26dp"

        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/return_button" />

</androidx.constraintlayout.widget.ConstraintLayout>